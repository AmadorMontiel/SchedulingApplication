<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="430.0" prefWidth="703.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View_Controller.ViewAppointmentsController">
   <children>
      <Label layoutX="14.0" layoutY="21.0" text="View Appointments">
         <font>
            <Font name="System Bold" size="14.0" />
         </font></Label>
      <TableView layoutX="5.0" layoutY="87.0" prefHeight="285.0" prefWidth="691.0">
        <columns>
          <TableColumn prefWidth="75.0" text="Appointment ID" />
          <TableColumn prefWidth="75.0" text="Title" />
            <TableColumn prefWidth="75.0" text="Description" />
            <TableColumn prefWidth="75.0" text="Location" />
            <TableColumn prefWidth="75.0" text="Contact" />
            <TableColumn prefWidth="75.0" text="Type" />
            <TableColumn prefWidth="75.0" text="Start Date and Time" />
            <TableColumn prefWidth="75.0" text="End Date and Time" />
            <TableColumn prefWidth="75.0" text="Customer ID" />
        </columns>
      </TableView>
      <RadioButton layoutX="207.0" layoutY="50.0" mnemonicParsing="false" selected="true" text="By Month">
         <toggleGroup>
            <ToggleGroup fx:id="timeToggleGroup" />
         </toggleGroup>
      </RadioButton>
      <RadioButton layoutX="392.0" layoutY="50.0" mnemonicParsing="false" text="By Week" toggleGroup="$timeToggleGroup" />
      <Button layoutX="638.0" layoutY="391.0" mnemonicParsing="false" onMouseClicked="#close" text="Exit" />
   </children>
</AnchorPane>
