<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="488.0" prefWidth="646.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View_Controller.AddCustomerController">
   <children>
      <Pane layoutX="15.0" layoutY="13.0" prefHeight="463.0" prefWidth="617.0" style="-fx-border-color: black; -fx-border-radius: 5;">
         <children>
            <Label layoutX="14.0" layoutY="14.0" text="Add Customer">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Label>
            <VBox layoutX="105.0" layoutY="59.0" spacing="15.0">
               <children>
                  <TextField fx:id="customerIDTextField" disable="true" />
                  <TextField fx:id="nameTextField" />
                  <TextField fx:id="addressTextField" />
                  <TextField fx:id="postalCodeTextField" />
                  <TextField fx:id="phoneNumberTextField" />
               </children>
            </VBox>
            <VBox layoutX="14.0" layoutY="59.0" prefHeight="165.0" prefWidth="81.0" spacing="25.0">
               <children>
                  <Label text="Customer ID" />
                  <Label text="Name" />
                  <Label text="Address" />
                  <Label text="Postal Code" />
                  <Label text="Phone Number" />
               </children>
            </VBox>
            <Button layoutX="405.0" layoutY="411.0" mnemonicParsing="false" onMouseClicked="#saveNewCustomer" text="Save" />
            <Button layoutX="487.0" layoutY="411.0" mnemonicParsing="false" onMouseClicked="#close" text="Cancel" />
            <ComboBox fx:id="countryComboBox" layoutX="425.0" layoutY="59.0" onAction="#countrySelection" prefWidth="150.0" />
            <Label layoutX="367.0" layoutY="63.0" text="Country" />
            <Label layoutX="367.0" layoutY="143.0" text="Division" />
            <ComboBox fx:id="firstLevelDivisionComboBox" layoutX="425.0" layoutY="139.0" prefWidth="150.0" />
         </children>
      </Pane>
   </children>
</AnchorPane>
